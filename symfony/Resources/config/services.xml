            </call>
            <call method="addObserver">
                <argument id="import_product_ee.observer.clean.up" type="service"/>
            </call>
        </service>
        <service id="import_product_ee.observer.composite.variant.add_update" class="TechDivision\Import\Observers\GenericCompositeObserver">
            <call method="addObserver">
                <argument id="import.observer.attribute.set" type="service"/>
            </call>
            <call method="addObserver">
                <argument id="import_product_variant_ee.observer.variant.super.link.update" type="service"/>
