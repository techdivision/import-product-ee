            </call>
        </service>
        <service id="import_product_ee.observer.composite.media.add_update" class="TechDivision\Import\Observers\GenericCompositeObserver">
            <call method="addObserver">
                <argument id="import.observer.attribute.set" type="service"/>
            </call>
            <call method="addObserver">
                <argument id="import_product_media_ee.media.gallery.update" type="service"/>
            </call>
            <call method="addObserver">
                <argument id="import_product_media_ee.media.gallery.value.update" type="service"/>
            </call>
        </service>
        <service id="import_product_ee.observer.composite.link.add_update" class="TechDivision\Import\Observers\GenericCompositeObserver">
            <call method="addObserver">
                <argument id="import_product_link_ee.observer.link" type="service"/>
            </call>
            <call method="addObserver">
                <argument id="import_product_link.observer.link.attribute.position.update" type="service"/>
            </call>
        </service>

        <service id="import_product_ee.subject.bunch" class="TechDivision\Import\Product\Ee\Subjects\EeBunchSubject" shared="false">
            <argument type="service" id="import.processor.registry"/>
            <argument type="service" id="import.generator.core.config.data.uid"/>
            <argument type="service" id="loggers"/>
            <argument type="service" id="import.events.emitter"/>
