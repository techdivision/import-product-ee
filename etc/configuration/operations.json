            }
          }
        },
        "add-update.media": {
          "plugins": {
            "subject": {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_product_media_ee.subject.media",
                  "file-resolver": {
                    "prefix": "media"
                  },
                  "params": {
                    "media-directory" : "pub/media/catalog/product",
                    "images-file-directory" : "var/importexport/media/catalog/product",
                    "clean-up-empty-columns": []
                  },
                  "observers": [
                    {
                      "import": [
